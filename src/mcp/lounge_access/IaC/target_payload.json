{
  "lambdaArn": "arn:aws:lambda:us-east-1:905418267822:function:atpco-lounge-access-mcp-us-east-1",
  "toolSchema": {
    "inlinePayload": [
      {
        "name": "search_lounges",
        "description": "Search for lounges for a specific airport",
        "inputSchema": {
          "type": "object",
          "properties": {
            "airport": {
              "type": "string",
              "description": "3-letter airport code (e.g., JFK, LAX)"
            },
            "username": {
              "type": "string",
              "description": "Username for personalization"
            }
          },
          "required": [
            "airport",
            "username"
          ]
        }
      },
      {
        "name": "get_user",
        "description": "Get user profile",
        "inputSchema": {
          "type": "object",
          "properties": {
            "username": {
              "type": "string",
              "description": "Username to get profile for"
            }
          },
          "required": [
            "username"
          ]
        }
      }
    ]
  }
}